          <table class="table mt-1">
            <thead class="form-group">
              <tr>
                <th scope="col">Name</th>
                <th scope="col">Email</th>
                <th scope="col">Mobile</th>
                <th scope="col">Amount</th>
                <th scope="col">Transact</th>
              </tr>
            </thead>
            <tbody>
              <% if(docs.length>0){%>
                <% docs.forEach((e) => {%>
                  <tr>
                    <td><%= e.nusername %></td>
                    <td><%= e.nemail %></td>
                    <td><%= e.mobile  %></td>
                    <td><%= e.amount  %></td>
                    <td>
                      <a href="/transact/<%=e._id %>" class="btn btn-primary">Transfer</a>
                    </td>
                  </tr>
                                
                  <%}) %>        
                
                <%}else{%>
                  <tr class="form-group">
                    <td>
                      
                        No records Found!
                      
                    </td>
                  </tr>
                
                  
                  <%} %>
            
            
            </tbody>
          </table>
        
       



</body>

</html>